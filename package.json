{
  "name": "mijn-kot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3001",
    "dev:d1": "npm run pages:build && npm run pages:dev",
    "dev:wrangler": "npx wrangler pages dev --d1=DB=mijnkot --compatibility-date=2024-06-20 --port 3000 --proxy 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2024-06-20 --d1=DB=mijnkot",
    "pages:deploy": "npx wrangler pages deploy .vercel/output/static",
    "d1:local:migrate": "wrangler d1 execute mijnkot --local --file=db/schema.d1.sql",
    "d1:remote:migrate": "wrangler d1 execute mijnkot --remote --file=db/schema.d1.sql",
    "seed": "node scripts/seed-d1.js",
    "seed:remote": "node scripts/seed-d1.js --remote"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.693.0",
    "@aws-sdk/s3-request-presigner": "3.693.0",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "2.49.1",
    "@tailwindcss/typography": "^0.5.19",
    "bcryptjs": "2.4.3",
    "clsx": "2.1.1",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jose": "5.9.6",
    "lucide-react": "^0.563.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^16.5.3",
    "react-markdown": "^10.1.0",
    "resend": "^6.9.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.13.15",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.3",
    "wrangler": "^4.60.0"
  }
}
